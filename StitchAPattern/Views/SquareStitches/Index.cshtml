@model IEnumerable<StitchAPattern.StitchingModels.SquareStitch>

@{
    ViewBag.Title = "Index";
}


@section myJavaScript{
<script>type="text/javascript" src="@Url.Content("~/Scripts/myJavaScript.js")"</script>
    }



<form method="get">

    <input type="search" name="SearchTerm" />
    <input type="submit" value="Search By Name" />

</form>

    <h2>Patterns</h2>
<div>
    @foreach (var item in Model)
    {
        <div id="patternContent" class="col-md-4 carousel">
            
            <h4>@item.Name</h4>
            <h4>@item.Description</h4>

            <div>
                <p>@item.MyPattern</p>
            </div>
           
                <a onclick="print()"href="#" class="btn btn-default">
                     <span class="glyphicon glyphicon-print"></span>
  
                </a>
            @*<div class="overlay">
                <hr />
            </div>*@

    </div>
    }
</div>
